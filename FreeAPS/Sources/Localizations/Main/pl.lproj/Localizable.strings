"Add Meal View settings " = "Dodaj ustawienia widoku posiłków ";

/* UI/UX option */
"Display Temp Targets Button" = "Wyświetlaj Przycisk Celów Tymczasowych";

/* UI/UX option */
"Home View Button Panel " = "Panel przycisków widoku ekranu głównego ";

/* UI/UX option */
"In case you're using both profiles and temp targets" = "W przypadku, gdy używasz zarówno profili, jak i celów tymczasowych";

/* UI/UX option */
"Always Color Glucose Value (green, yellow etc)" = "Zawsze kolorowa wartość glukozy (zielony, żółty itp.)";

/* UI/UX option */
"Header settings" = "Ustawienia nagłówków";
